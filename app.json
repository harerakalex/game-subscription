{
  "expo": {
    "name": "diver-ads",
    "slug": "game-advertising",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bytefusion.gameadvertising"
    },
    "android": {
      "package": "com.bytefusion.gameadvertising",
      "permissions": [
        "NOTIFICATIONS"
      ],
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
